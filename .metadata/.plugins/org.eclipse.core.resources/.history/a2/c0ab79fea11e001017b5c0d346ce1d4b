package core;
//single
class A{
	public void classA() {
		System.out.println("A class");
	}
}
class B extends A{
	public void classB() {
		System.out.println("B class");
	}
}
//multilevel
class C{
	
}
public class P012_Inheritance {
	public static void main(String[] args) {
		B b = new B();
		b.classA();
		b.classB();
	}
}
