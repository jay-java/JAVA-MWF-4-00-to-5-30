package core;
class Student{
	int id;
	String name;
	long contact;
	static String col_name = "XYZ";
	public Student(int id,String name,long contact) {
		this.id = id;
		this.name = name;
	public void show() {
		System.out.println("id : "+id+" name : "+name+" contact: "+contact+" col_name : "+col_name);
	}
}
public class P015_Static {
	public static void main(String[] args) {
		Student s
	}
}
