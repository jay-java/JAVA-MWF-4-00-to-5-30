package core;

class Overload {
	public int add(int a, int b) {
		return a+b;
	}

	public int add(int a,int b ,int c) {
		return a+b+c;
	}
}

class Override extends Overload{
	public int add(int a, int b) {
		return a+b;
	}
}

public class P013_Polymorphism {
	public static void main(String[] args) {
		Overload o = new Overload();
		int res1 = o.add(1,2);
		int res2 = o.add(1, 2, 3);
	}
}
