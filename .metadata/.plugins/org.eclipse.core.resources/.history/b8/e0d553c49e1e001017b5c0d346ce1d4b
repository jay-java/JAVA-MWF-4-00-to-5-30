package core;
class User{
	int id;
	String name;
	String address;
	long contact;
	public User() {
		System.out.println("this default cons");
	}
	public User() {
		System.out.println();
	}
	public void show() {
		System.out.println("show fun in User");
	}
	
}
public class P011_ConstructorAndThis {
	public static void main(String[] args) {
		User u = new User();
		u.show();
		User u1 = new User();
	}
}
